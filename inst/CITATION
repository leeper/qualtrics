citHeader("To cite package 'qualtrics' in publications use:")
     
 year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", meta$Date, perl = TRUE)
 vers <- paste("R package version", meta$Version)
 
 citEntry(entry="Manual",
		  title = "qualtrics: Qualtrics Survey Data Access",
		  author = personList(as.person("Jason Bryer")),
		  year = year,
		  note = vers,
		  textVersion =
		  paste("Jason Bryer (",
				year,
				"). qualtrics: Qualtrics Survey Data Access. ",
				vers, ".", sep=""))
